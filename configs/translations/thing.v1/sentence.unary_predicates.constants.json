      "<<phrase::if_conjunction>> <<sentence::{A}{a}>> <<phrase::maybe_then>> <<sentence::¬(¬{B}it v ¬{C}it)>>",
      "<<sentence::¬(¬{B}{a} v ¬{C}{a})>> <<phrase::if_conjunction>> <<sentence::{A}it>>"
    ],
    "¬{A}{a} -> ¬(¬{B}{a} v ¬{C}{a})": [
      "<<phrase::if_conjunction>> <<sentence::¬{A}{a}>> <<phrase::maybe_then>> <<sentence::¬(¬{B}it v ¬{C}it)>>",
      "<<sentence::¬(¬{B}{a} v ¬{C}{a})>> <<phrase::if_conjunction>> <<sentence::¬{A}it>>"
    ]


  }
  
}
