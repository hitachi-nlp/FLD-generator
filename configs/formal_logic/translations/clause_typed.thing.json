{
  "translations": {
    "{A}{a}": [
      "{verb_clause.{A}{a}}."
    ],
    "¬{A}{a}": [
      "{verb_clause.¬{A}{a}}."
    ],
    "({A} & {B}){a}": [
      "{verb_clause.({A} & {B}){a}}."
    ],
    "({A} v {B}){a}": [
      "{verb_clause.({A} v {B}){a}}."
    ],

    "(x): {A}x -> {B}x": [
      "if {verb_clause.{A}x}, then {verb_clause.{B}one}."
    ],
    "(x): ({A} & {B})x -> {C}x": [
      "if {verb_clause.({A} & {B})x}, then {verb_clause.{C}one}."
    ]
  },

  "clauses": {
    "verb_clause": {
      "{A}{a}": [
        "{a} is {A}[ADJ]",
        "{a} is {A}[VERB]",
        "{a} {A}[VERB.s]"
      ],
      "¬{A}{a}": [
        "{a} is not {A}[ADJ]",
        "{a} is not {A}[VERB]",
        "{a} does not {A}[VERB]"
      ],
      "({A} & {B}){a}": [
          "{a} is both {A}[ADJ] and {B}[ADJ]",
          "{a} is {A}[ADJ] and {B}[VERB.s]",
          "{a} {A}[VERB.s] and is {B}[ADJ]",
          "{a} {A}[VERB.s] and {B}[VERB.s]"
      ],
      "{A}x": [
        "something is {A}[ADJ]",
        "something {A}[VERB.s]"
      ],
      "{A}one": [
        "it is {A}[ADJ]",
        "it {A}[VERB.s]"
      ],
      "({A} & {B})x": [
        "something is both {A}[ADJ] and {B}[ADJ]",
        "something {A}[VERB.s] and is {B}[ADJ]",
        "something is {A}[ADJ] and is {B}[VERB.s]",
        "something both {A}[VERB.s] and is {B}[VERB.s]"
      ]
    },
    "noun_clause": {
      "{A}{a}": [
        "{A}[ADJ] {a}",
        "{A}[VERB.ing] {a}",
        "{a} who {A}[VERB.s]"
      ],
      "{A}x": [
        "{A}[ADJ] one",
        "{A}[VERB.ing] one",
        "something who {A}[VERB.s]"
      ],

      "{A}x": [
        "{A}[ADJ] one",
        "{A}[VERB.ing] one",
        "something who {A}[VERB.s]"
      ],

      "{A}": [
        "{A}[ADJ.ness]",
        "{A}[VERB.ing]",
        "{A}[NOUN]"
      ]
    }
  }
}
